{
  "name": "server",
  "version": "3.0.0",
  "private": true,
  "scripts": {
    "start": "node server/bin/www",
    "build": "cd react-ui/ && npm install && npm run build",
    "dev": "nodemon server/bin/www",
    "heroku-postbuild": "NPM_CONFIG_PRODUCTION=false npm install --prefix client && npm run build --prefix client"
  },
  "dependencies": {
    "cheerio": "^1.0.0-rc.3",
    "cookie-parser": "~1.4.4",
    "cors": "^2.8.5",
    "debug": "~2.6.9",
    "dotenv": "^8.2.0",
    "express": "~4.16.1",
    "helmet": "^3.21.2",
    "http-errors": "~1.6.3",
    "jade": "~1.11.0",
    "morgan": "~1.9.1",
    "pg": "^7.14.0",
    "request": "^2.88.0",
    "request-promise": "^4.2.5"
  },
  "cacheDirectories": [
    "node_modules",
    "client/node_modules"
  ],
  "keywords": [
    "node",
    "heroku",
    "create-react-app",
    "react"
  ],
  "license": "MIT",
  "devDependencies": {
    "dotenv": "^8.2.0",
    "nodemon": "^1.19.3",
    "sequelize-cli": "^5.5.1"
  },
  "nodemonConfig": {
    "ignore": [
      "/client"
    ]
  }
}
